package snippet;

public class Snippet {
	//자신이 작성한 리뷰를 젤 최상단으로 - 시작
				 int index = -1;
					// 'getTrainerprofile_u_key'가 memberprofile_u_key인 요소를 찾아서 인덱스를 가져옴
					 for (int i = 0; i < reviewlist.size(); i++) {
					     if (reviewlist.get(i).getMemberprofile_u_key() == memberprofile_u_key) {
					         index = i;
					         break;
					     }
					 }
					 // index가 유효한 경우에만 요소 이동 수행
					 if (index != -1 && index != 0) {
					     Review targetReview = reviewlist.get(index);
					     reviewlist.remove(index);
					     reviewlist.add(0, targetReview);
					 }
					 //자신이 작성한 리뷰를 젤 최상단으로 - 끝
}

