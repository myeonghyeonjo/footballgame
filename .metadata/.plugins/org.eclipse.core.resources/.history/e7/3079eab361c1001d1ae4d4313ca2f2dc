<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zikgu.example.mapper.PlayerMapper">


	<select id="selectplayerList" resultType="Player">
		SELECT id as id,
		team as team,
		shirt as shirt,
		name as name,
		age as age,
		pos as pos

		FROM fbplayer
	</select>

	<select id="selectPlayerListOne" resultType="Player">
		SELECT id as id,
		team as team,
		shirt as shirt,
		name as name,
		age as age,
		pos as pos

		FROM fbplayer
		WHERE id = #{c_id}
	</select>

</mapper>


